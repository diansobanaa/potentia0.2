# =============================================================
#            File Konfigurasi Lingkungan Contoh
# =============================================================
# 
# Salin file ini ke ".env" dan isi semua nilai yang kosong.
# Jangan pernah membagikan file .env Anda yang sudah terisi.
# 

# Kredensial Supabase 
# Ambil ini dari dashboard Supabase > Project Settings > API
SUPABASE_URL="https://YOUR_PROJECT_ID.supabase.co"
SUPABASE_ANON_KEY=""
SUPABASE_SERVICE_ROLE_KEY=""

# ------------------------------
# Ambil dari dashboard Supabase > Project Settings > Database > Connection string
DATABASE_URL="postgresql://postgres.YOUR_PROJECT_ID:YOUR_DATABASE_PASSWORD@aws-1-ap-southeast-1.pooler.supabase.com:6543/postgres"
# ------------------------------

# Rahasia Aplikasi 
# Hasilkan kunci acak yang kuat (misalnya, menggunakan 'openssl rand -base64 64')
JWT_SECRET=""
# Ambil dari dashboard Supabase > Project Settings > API > JWT Secret
SUPABASE_JWT_SECRET=""

# URL Redis 
# 'potentia_redis' adalah nama service dari docker-compose.yml
REDIS_URL="redis://potentia_redis:6379"

# [BARU] External Tools (Fase 2)
# Opsional, untuk fungsi pencarian web
TAVILY_API_KEY=""

# [BARU] Feature Flag (Fase 2)
# Set ke 1.0 untuk mengaktifkan LangGraph v2.1 sepenuhnya
LANGGRAPH_ROLLOUT_PERCENT=1.0

# RAG Service (Tidak Berubah)
# ID ini mungkin perlu diatur agar sesuai dengan data di database Anda
DEFAULT_ROLE_ID="e9d0bd8d-9f55-4845-90f2-e99220faea28"
SEEDING_ADMIN_KEY="kunci_rahasia_anda_yang_sangat_panjang_dan_acak"
AI_AGENT_USER_ID="d9856c1e-87ed-4f86-9ecc-4af822a08f92"


# ===== LLM Configuration =====
# Model spesifik backend (RAG, Asesor). Ini *BUKAN* untuk chat.
GEMINI_RERANKER_MODEL="gemini-flash-lite-latest"
GEMINI_ASESOR_MODEL="gemini-flash-lite-latest"

# Default fallback model (jika frontend tidak mengirim llm_config)
DEFAULT_MODEL="gemini-2.5-flash"
DEFAULT_TEMPERATURE=0.2

# --- Kredensial Provider (Hanya API Key & Base URL) ---
GEMINI_API_KEY=""
OPENAI_API_KEY=""
OPENAI_BASE_URL="https://api.openai.com/v1"
DEEPSEEK_API_KEY=""
DEEPSEEK_BASE_URL="https://api.deepseek.com"
KIMI_API_KEY=""
KIMI_BASE_URL="https://api.moonshot.cn/v1"
XAI_API_KEY=""
XAI_BASE_URL="https://api.x.ai/v1/chat/completions"

# Fallback defaults (used only if llm_config not provided)
DEFAULT_MODEL="gemini-2.5-flash"
DEFAULT_TEMPERATURE=0.2